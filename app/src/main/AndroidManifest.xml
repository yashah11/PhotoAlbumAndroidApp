<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs213.andriodPhoto03">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cs213.andriodPhoto03.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cs213.andriodPhoto03.AlbumActivity"
            android:parentActivityName="cs213.andriodPhoto03.MainActivity" />
        <activity
            android:name="cs213.andriodPhoto03.PhotoActivity"
            android:parentActivityName="cs213.andriodPhoto03.AlbumActivity" />
    </application>

</manifest>